package chapter09.cookbook;

public class Ingredient {
   private FoodItem item;
   private int qty;
   
   public Ingredient(FoodItem item, int qty) {
      this.item = item;
      this.qty = qty;
   }
   
   public FoodItem getItem() {
      return item;
   }
   
   public int getQty() {
      return qty;
   }
   
}
