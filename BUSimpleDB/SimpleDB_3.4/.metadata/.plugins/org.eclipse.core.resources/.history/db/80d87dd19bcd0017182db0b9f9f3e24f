package bank.version17;
//Collateral ratio uses the strategy pattern

public class InterestChecking extends CheckingAccount {

   public InterestChecking(int acctnum, boolean isforeign, boolean ispreferred) {
      super(acctnum, isforeign, ispreferred);
   }

   protected String accountType() {
      return "Interest Checking";
   }
   
   protected double interestRate() {
      return 0.01;
   }
}

