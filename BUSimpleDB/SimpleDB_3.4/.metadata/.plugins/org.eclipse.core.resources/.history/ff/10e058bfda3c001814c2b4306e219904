package hw7;

import java.util.Iterator;

/**
 *
 * @author aleja
 */
public class LookAheadIterator<T> implements Iterator<T> {
    
    Iterator<T> it;
    T current;
    T next;
    public LookAheadIterator(Iterator<T> iter){
        this.it=iter;
        this.next = iter.next();
    }

    @Override
    public boolean hasNext(){
        return !(next==null);
    }

    @Override
    public T next() {
        current=next;
        next = it.hasNext() ? it.next() : null;
        return current;
    }
    
    public T peek(){
        return next;
    }
    
    
}
